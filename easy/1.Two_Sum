func twoSum(nums []int, target int) []int {

    var indices []int;
    for i:=0 ; i<len(nums)-1;i++{
        for j:=i+1;j<len(nums);j++{
            if nums[j]+nums[i] == target {
                indices=append(indices,j,i)
                return indices
            }
                
        }

    }
    return indices

}
